{
	"compilerOptions": {
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "es2022",
		"allowJs": true,
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"moduleDetection": "force",
		"isolatedModules": true,
		// "verbatimModuleSyntax": true,
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitOverride": true
	}
}
